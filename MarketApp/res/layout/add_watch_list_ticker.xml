<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:id="@+id/RelativeLayout01"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android">
	<TextView android:id="@+id/add_watch_list_ticker_label"
		android:text="Enter Ticker" android:layout_width="wrap_content"
		android:layout_height="wrap_content"/>
	<EditText android:layout_height="wrap_content"
		android:layout_width="200dip" android:id="@+id/add_watch_list_ticker_text"
		android:layout_below="@id/add_watch_list_ticker_label"
		android:layout_alignParentLeft="true"
		android:singleLine="true"
		android:nextFocusUp="@+id/add_watch_list_ticker_add_button" />
	<Button android:layout_height="wrap_content"
		android:layout_width="wrap_content" android:text="Add"
		android:id="@+id/add_watch_list_ticker_add_button"
		android:layout_alignParentRight="true" 
		android:layout_toRightOf="@+id/add_watch_list_ticker_text"
		android:layout_alignTop="@+id/add_watch_list_ticker_text" />
</RelativeLayout>
