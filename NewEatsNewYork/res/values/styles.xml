<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="DiningDay" parent="CodeFont">
        <item name="android:textColor">#000000</item>
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_height">80dip</item>
        <item name="android:background">@drawable/notepad</item>
        <item name="android:gravity">center</item>
        <item name="android:typeface">monospace</item>
    </style>

    <style name="CodeFont">
        <item name="android:textColor">#000000</item>
        <item name="android:layout_width">120dip</item>
        <item name="android:layout_height">80dip</item>
        <item name="android:gravity">center</item>
        <item name="android:typeface">monospace</item>
    </style>

    <style name="ToggleCountry">
        <item name="android:disabledAlpha">?android:attr/disabledAlpha</item>
        <item name="android:textOn">@null</item>
        <item name="android:textOff">@null</item>
        <item name="android:background">@null</item>
    </style>

    <style name="ToggleCountry.China">
        <item name="android:drawableTop">@drawable/selector_china</item>
    </style>

    <style name="ToggleCountry.Africa">
        <item name="android:drawableTop">@drawable/selector_africa</item>
